<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="Openworx Backend Theme" inherit_id="web.assets_backend">
        <xpath expr=".">
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/style.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/sidebar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/boostrap.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/navbar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/modal.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/progress_bar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/form_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/form_view_extra.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/list_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/kanban_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/fields_extra.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/webclient_extra.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/search_view_mobile.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/chatter.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/followers.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/thread.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/systray.scss"/>
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/web_responsive.scss"/>
            <script type="application/javascript" src="/backend_theme_v12/static/src/js/sidebar.js"/>
            <script type="application/javascript" src="/backend_theme_v12/static/src/js/sidebar-toggle.js"/>
        </xpath>
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/layout.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/backend_theme_v12/static/src/js/views/form_renderer.js" />
        </xpath>
    </template>

    <template id="_assets_primary_variables" inherit_id="web._assets_primary_variables">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/primary_variables.scss"/>
        </xpath>
    </template>

    <template id="_assets_secondary_variables" inherit_id="web._assets_secondary_variables">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/backend_theme_v12/static/src/scss/secondary_variables.scss"/>
        </xpath>
    </template>

</odoo>
